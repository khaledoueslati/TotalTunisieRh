<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('icone-360.ico') }}" />
        <link href="{{ asset('bundles/AppT360/css/font-awesome.min.css') }}" rel="stylesheet" type="text/css"/>
        <link href="{{ asset('bundles/AppT360/css/AdminLTE.min.css') }}" rel="stylesheet" type="text/css"/>
        <link href="{{ asset('bundles/AppT360/css/bootstrap.min.css') }}" rel="stylesheet" type="text/css"/>
    </head>
    <body background="{{ asset('bundles/AppT360/images/rh_background.jpg') }}">

    <div class="login-box">

        <div class="login-box-body">
            {% block body %}
            {% endblock %}

        </div><!-- /.login-box-body -->
    </div><!-- /.login-box -->

    <!-- jQuery 2.1.3 -->
    <script src="{{ asset('bundles/AppT360/js/jQuery-2.1.3.min.js') }}"></script>
    <!-- Bootstrap 3.3.2 JS -->
    <script src="{{ asset('bundles/AppT360/js/bootstrap.min.js') }}" type="text/javascript"></script>

    {% block javascripts %}{% endblock %}
    </body>
</html>
