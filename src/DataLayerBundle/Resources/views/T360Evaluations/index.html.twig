{% extends '::base.html.twig' %}

{% block body -%}
    <h1>T360Evaluations list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Datedebut</th>
                <th>Datefin</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('t360evaluations_show', { 'id': entity.idEvaluation }) }}">{{ entity.idEvaluation }}</a></td>
                <td>{{ entity.dateDebut }}</td>
                <td>{{ entity.dateFin }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('t360evaluations_show', { 'id': entity.idEvaluation }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('t360evaluations_edit', { 'id': entity.idEvaluation }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('t360evaluations_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
