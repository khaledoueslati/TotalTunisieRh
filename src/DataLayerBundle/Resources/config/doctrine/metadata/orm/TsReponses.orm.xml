<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="TsReponses" table="ts_reponses">
    <indexes>
      <index name="fk_Reponses_Choix1_idx" columns="Choix_idChoix"/>
      <index name="fk_Reponses_Questions1_idx" columns="Questions_idQuestion"/>
      <index name="fk_TS_Reponses_TS_Jetons1_idx" columns="TS_Jetons_idTS_Jeton"/>
    </indexes>
    <id name="idreponse" type="integer" column="idReponse">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="date" type="string" column="Date" length="10" nullable="true"/>
    <many-to-one field="tsJetonstsJeton" target-entity="Jetons">
      <join-columns>
        <join-column name="TS_Jetons_idTS_Jeton" referenced-column-name="idTS_Jeton"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="questionsquestion" target-entity="TsQuestions">
      <join-columns>
        <join-column name="Questions_idQuestion" referenced-column-name="idQuestion"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="choixchoix" target-entity="TsChoix">
      <join-columns>
        <join-column name="Choix_idChoix" referenced-column-name="idChoix"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
